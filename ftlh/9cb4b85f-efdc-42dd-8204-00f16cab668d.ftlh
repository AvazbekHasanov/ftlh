      @media print {
        body,
        page {
          margin: 0;
          box-shadow: 0 0 0 transparent;
        }

        @page {
          size: A4;
          margin: 0;
        }
      }

      * {
        margin: 0;
        padding: 0;
        font-size: 18px;
        line-height: 1.15;
      }

      body {
        background: #ccc;
      }

      page {
        background: #fff;
        display: block;
